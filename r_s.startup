#!/bin/bash
ip a add 172.16.202.13/22 dev eth0
ip a add 172.16.192.3/23 dev eth1
ip link set eth0 up
ip link set eth1 up 
ip route add default via 172.16.192.5
ip route add 172.16.195.0/22 via 172.16.192.5   
ip route add 172.16.196.0/24 via 172.16.192.5   
ip route add 172.16.197.0/23 via 172.16.192.5       
echo 1 > /proc/sys/net/ipv4/ip_forward


